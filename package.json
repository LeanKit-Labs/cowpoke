{
  "name": "cowpoke",
  "version": "0.6.0",
  "description": "A service to handle configurable Rancher service upgrade patterns",
  "engines": {
    "node": " >=6.0.0"
  },
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "mocha": "mocha spec/**",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha --compilers js:babel-register -- spec/** --reporter dot",
    "test": "npm run mocha && npm run coverage",
    "run": "node src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/arobson/cowpoke.git"
  },
  "keywords": [
    "rancher",
    "upgrade",
    "automation"
  ],
  "author": "Alex Robson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arobson/cowpoke/issues"
  },
  "homepage": "https://github.com/arobson/cowpoke#readme",
  "dependencies": {
    "@slack/client": "^3.3.0",
    "autohost": "^0.6.0",
    "bluebird": "^3.4.0",
    "configya": "^0.2.2",
    "fount": "^0.1.0",
    "request": "^2.67.0",
    "request-promise": "^3.0.0",
    "urlencode": "^1.1.0",
    "lodash": "^3.10.1",
    "hyped": "^0.5.1",
    "js-yaml": "^3.6.1"
  },
  "devDependencies": {
    "proxyquire": "^1.7.4",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "eslint": "^3.0.1",
    "isparta": "^4.0.0",
    "mocha": "^2.5.3",
    "sinon": "^1.17.2",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0"
  }
}
